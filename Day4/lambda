# print(add_two(1,2))

numbers = [1,2,3,4,5,6,7,8]

mult_2 = lambda num: num * 2 
result_list = list(map(mult_2, numbers))
print(result_list)

#2
result_list = list(map(lambda num: num * 2, numbers))
print(result_list)

words = ['dog', 'can', 'ball']

capit = list(map(lambda s: s.capitalize(), words))
print(capit)
